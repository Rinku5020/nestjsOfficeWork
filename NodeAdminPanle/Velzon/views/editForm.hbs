



<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <form action="/user/createForm" method="POST">
  <input type="text" name="FirstName" placeholder="First Name" required />
  <br><br>

  <input type="text" name="LastName" placeholder="Last Name" required />
  <br><br>
  <input type="email" name="Email" placeholder="Email" required />
  <br><br>
  <input type="text" name="Phone" placeholder="Phone" required />
  <br><br>

  <!-- Gender -->
  <label><input type="radio" name="gender" value="male" /> Male</label>
  <label><input type="radio" name="gender" value="female" /> Female</label>
  <br><br>

  <!-- Hobbies -->
  <label><input type="checkbox" name="hobbies" value="playing" /> Playing</label>
  <label><input type="checkbox" name="hobbies" value="reading" /> Reading</label>
  <label><input type="checkbox" name="hobbies" value="travelling" /> Travelling</label>
  <br><br>

  <input type="date" name="dateOfBirth" required />
  <br><br>
  <textarea name="address" required></textarea>
  <br><br>
  <input type="text" name="image" placeholder="Image path or URL" required />
  <br><br>

  <button type="submit">Submit</button>
</form>

</body>
</html>